    <item rdf:about="http://zipofar.ru/doku.php?id=config_php&amp;rev=1510059695&amp;do=diff">
        <dc:format>text/html</dc:format>
        <dc:date>2017-11-07T16:01:35+03:00</dc:date>
        <dc:creator>ingprog</dc:creator>
        <title>config_php</title>
        <link>http://zipofar.ru/doku.php?id=config_php&amp;rev=1510059695&amp;do=diff</link>
        <description>Config for PHP


server {
        listen 80;
        root /var/www/wp1.loc;
        index index.php index.html;
        server_name wp1.loc;

        location / {
                try_files $uri $uri/ /index.php$args;
        }

        location ~ \.php$ {
                include snippets/fastcgi-php.conf;
                fastcgi_pass 10.0.3.238:9000;
        }

}</description>
    </item>
